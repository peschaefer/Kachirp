<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="533.0" prefWidth="800.0" stylesheets="@styles.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.bsu.cs222.GUIControllers.BankSelectController">
   <VBox alignment="CENTER" layoutX="169.0" layoutY="89.0" prefHeight="356.0" prefWidth="462.0">
      <Label text="Question Bank Selector">
         <font>
            <Font size="45.0" />
         </font>
         <VBox.margin>
            <Insets bottom="15.0" top="15.0" />
         </VBox.margin>
      </Label>
      <Label text="Please Select Your Question Bank!">
         <font>
            <Font size="22.0" />
         </font>
         <VBox.margin>
            <Insets top="10.0" />
         </VBox.margin>
      </Label>
      <ComboBox fx:id="bankComboBox" onAction="#enableSubmitButton" prefHeight="38.0" prefWidth="251.0">
         <VBox.margin>
            <Insets top="10.0" />
         </VBox.margin>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></ComboBox>
      <Button fx:id="submitButton" disable="true" mnemonicParsing="false" onAction="#chooseBank" text="Submit Bank">
         <font>
            <Font size="15.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <VBox.margin>
            <Insets top="10.0" />
         </VBox.margin>
      </Button>
   </VBox>
</AnchorPane>
